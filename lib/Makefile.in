srcdir = @srcdir@
VPATH = $(srcdir)

CC = @CC@
CFLAGS = @CFLAGS@
LDFLAGS = @LDFLAGS@
LIBS = @LIBS@
RANLIB = @RANLIB@

LIB = libzile.a
OBJS = alist.o astr.o htable.o @LIBOBJS@

.c.o:
	$(CC) -c -I. -I.. -I$(srcdir) $(CFLAGS) $<

all: $(LIB)

$(LIB): $(OBJS)
	rm -f $(LIB)
	ar rc $(LIB) $(OBJS)
	$(RANLIB) $(LIB)

.PHONY: install uninstall clean realclean distclean mostlyclean

install:

uninstall:

clean: 
	-rm -f $(LIB) *.o

distclean: clean 
	-rm -f Makefile

realclean: distclean

mostlyclean: clean

$(OBJS): ../config.h
alist.o: alist.h
astr.o: astr.h
htable.o: htable.h
