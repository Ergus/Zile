#	$Id: Makefile.in,v 1.2 2003/04/24 15:11:15 rrt Exp $

subdirs = lib doc etc src

all install uninstall clean realclean mostlyclean:
	@for f in $(subdirs); do \
		(cd $$f && $(MAKE) $@); \
	done

distclean:
	@for f in $(subdirs); do \
		(cd $$f && $(MAKE) $@); \
	done
	-rm -f Makefile config.cache config.status config.log
