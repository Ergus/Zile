#	$Id: Makefile.in,v 1.3 2003/05/06 22:28:39 rrt Exp $

srcdir = @srcdir@
VPATH = $(srcdir)

subdirs = lib doc etc src

all install uninstall clean realclean mostlyclean:
	@for f in $(subdirs); do \
		(cd $$f && $(MAKE) $@); \
	done

distclean:
	@for f in $(subdirs); do \
		(cd $$f && $(MAKE) $@); \
	done
	-rm -f Makefile config.cache config.status config.log config.h
